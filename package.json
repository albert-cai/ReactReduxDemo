{
  "name": "angulares6",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/nodemon --exec better-npm-run startdev",
    "test": "npm test",
    "clean": "rimraf dist",
    "buildserver": "rimraf serverDist && babel @server -d serverDist -s --presets es2015,stage-3",
    "startprod": "rimraf dist && better-npm-run build:server:prod && better-npm-run build:web:prod && better-npm-run start:prod"
  },
  "betterScripts": {
    "build:server:prod": {
      "command": "webpack --config webpack.config/server.config.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:web:prod": {
      "command": "webpack --config webpack.config/web.prod.config.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start:prod": {
      "command": "node dist/server.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "startdev": {
      "command": "babel-node @server/bin/server.js --presets es2015,stage-3",
      "env": {
        "NODE_ENV": "DEV"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wssjq198913/AngularES6.git"
  },
  "keywords": [
    "Angular1.x",
    "ES6"
  ],
  "author": "Johnny",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wssjq198913/AngularES6/issues"
  },
  "homepage": "https://github.com/wssjq198913/AngularES6#readme",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "cookie-session": "^1.3.0",
    "csurf": "^1.9.0",
    "debug": "~2.6.3",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "express-session": "^1.15.4",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "~1.8.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
